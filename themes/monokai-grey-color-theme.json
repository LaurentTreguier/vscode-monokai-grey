    "tokenColors": [
        {
            "settings": {
                "foreground": "#dfdfdf",
                "background": "#252729"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#7f8f9f"
            }
        },
        {
            "name": "Constant",
            "scope": "constant",
            "settings": {
                "foreground": "#9f7fff"
            }
        },
        {
            "name": "Type name",
            "scope": "entity.name",
            "settings": {
                "foreground": "#9fdf2f"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#ff1f7f"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#9fdf2f"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#9fdf2f"
            }
        },
        {
            "name": "Class and struct name",
            "scope": [
                "entity.name.class",
                "entity.name.struct",
                "entity.name.type.class",
                "entity.name.type.struct"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "background": "#ff1f7f"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "background": "#9f7fff"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#ff1f7f"
            }
        },
        {
            "name": "Markup changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#dfdf00"
            }
        },
        {
            "name": "Markup deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ff3f00"
            }
        },
        {
            "name": "Markup headings",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#9fdf2f"
            }
        },
        {
            "name": "Markup inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#3fff00"
            }
        },
        {
            "name": "Markup list",
            "scope": "markup.list",
            "settings": {
                "foreground": "#dfdf7f"
            }
        },
        {
            "name": "Markup quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#ff1f7f"
            }
        },
        {
            "name": "Markup styling",
            "scope": [
                "markup.bold",
                "markup.italic"
            ],
            "settings": {
                "foreground": "#5fdfff"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#ff9f1f"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#ff1f7f"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5fdfff"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#dfdf7f"
            }
        },
        {
            "name": "Support",
            "scope": [
                "support.constant",
                "support.function",
                "support.variable"
            ],
            "settings": {
                "foreground": "#5fdfff"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.class",
                "support.type"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5fdfff"
            }
        },
        {
            "name": "Variable language",
            "scope": "variable.language",
            "settings": {
                "foreground": "#ff1f7f"
            }
        },
        {
            "name": "Variable parameter",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ff9f1f"
            }
        },
        // language specific tweaks
        {
            "name": "Template - Javascript, Typescript",
            "scope": "meta.template.expression",
            "settings": {
                "foreground": "#dfdfdf"
            }
        },
        {
            "name": "Template - Javascript, Typescript",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#9f7fff"
            }
        },
        {
            "name": "String - JSON",
            "scope": "string.quoted.double.json",
